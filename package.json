{
  "name": "promised-rest",
  "version": "2.0.0",
  "description": "Boilerplate API for study",
  "main": "app.js",
  "author": "Mithun Das <mithun.das@jumacro.com>",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=4.6.0",
    "npm": ">=2.15.9",
    "yarn": ">=0.20.0"
  },
  "scripts": {
    "start": "gulp serve",
    "build": "gulp",
    "lint": "esw *.js server config --color",
    "lint:watch": "yarn lint -- --watch",
    "lint:fix": "yarn lint -- --fix",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --ui bdd --reporter spec --colors --compilers js:babel-core/register ./api/**/*.test.js",
    "test:watch": "yarn test -- --watch",
    "test:coverage": "cross-env NODE_ENV=test ./node_modules/.bin/istanbul cover _mocha -- --ui bdd --reporter spec --colors --compilers js:babel-core/register ./api/**/*.test.js",
    "test:check-coverage": "yarn test:coverage && istanbul check-coverage",
    "report-coverage": "coveralls < ./coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jumacro/PromisedRest.git"
  },
  "dependencies": {
   "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.3",
    "crypto-js": "^3.1.9-1",
    "debug": "^2.6.8",
    "express": "^4.15.3",
    "express-winston": "^2.4.0",
    "helmet": "^3.6.1",
    "http-status": "^1.0.1",
    "jsonfile": "4.0.0",
    "jsonwebtoken": "^7.4.1",
    "method-override": "^2.3.9",
    "mongoose": "^4.10.2",
    "morgan": "^1.8.2",
    "multer": "^1.3.0",
    "multiparty": "^4.1.3",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "request": "^2.81.0",
    "request-promise": "^4.2.1",
    "uniqid": "^4.1.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.16",
    "cross-env": "^5.0.0",
    "del": "^2.2.2",
    "eslint": "^3.15.0",
    "eslint-config-airbnb-base": "^11.1.0",
    "eslint-fix": "^1.0.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-watch": "^3.1.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-load-plugins": "^1.5.0",
    "gulp-newer": "^1.3.0",
    "gulp-nodemon": "^2.2.1",
    "gulp-sourcemaps": "^2.4.1",
    "gulp-util": "^3.0.8",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "run-sequence": "^1.2.2",
    "supertest": "^3.0.0",
    "validate-commit-msg": "^2.11.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "add-module-exports"
    ]
  }
}
